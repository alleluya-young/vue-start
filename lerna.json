{
  "packages": ["@vue-start/*", "@vue-start-dev/*"],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "ignoreChanges": ["**/*.md", "**/*.js"],
  "command": {
    "version": {
      "exact": false,
      "allowBranch": "main",
      "message": "chore(release): %s"
    }
  },
  "version": "independent"
}
